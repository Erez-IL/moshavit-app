<!--<h2>New User</h2>-->
<div class="well span12" xmlns="http://www.w3.org/1999/html" xmlns="http://www.w3.org/1999/html"
     xmlns="http://www.w3.org/1999/html">
    <!--UserName-->
    <div class="input-prepend">
        <label for="username" >UserName</label>
        <input type="text" style="margin-top:-5px;" class="username" value="" >
    </div>
    <!-- E-mail -->
    <div class="input-prepend">
        <label for="email">E-Mail</label>
        <input type="email" class="email" value="" placeholder="Email" required="required" >
    </div>
    <!-- First Name -->
    <div class="input-prepend">
        <label for="firstName">FirstName</label>
        <input type="text" class="firstName" value="" placeholder="First name" >
    </div>
    <!-- Last Name -->
    <div class="input-prepend">
        <label for="lastName">LastName</label>
        <input type="text" class="lastName" value="" placeholder="Last name" >
    </div>
    <!-- Membership Type-->
    <div class="input-prepend">
        <label for="membership">Membership</label>
        <input type="text" class="membership" value="" placeholder="Membership" >
    </div>
		<div class="input-prepend">
      <label for="password">password</label>
      <input id="password" type="password" class="password" value="">
  </div>
		<div class="input-prepend">
      <label for="passwordConfirm">password Confirm</label>
      <input id="passwordConfirm" type="password" class="passwordConfirm" value="">
  </div>
		<button class="btn clear">Clear</button>
  <button class="btn register">Register</button>
</div>
